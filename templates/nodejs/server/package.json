{
    "name": "{{info.name}}",
    "version": "{{info.version}}",
    "description": "",
    "repository": "{{{info.repository}}}",
    "license": "",
    "author": "{{{info.author}}}",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "yarn lint && yarn cibuild",
        "cibuild": "echo building && tsc",
        "debug": "nodemon --legacy-watch --watch 'src/**/*' --ext 'ts' --exec node --inspect=0.0.0.0:9229 --require ts-node/register ./src/server.ts",
        "docs": "typedoc --plugin typedoc-plugin-markdown --out docs/reference --target esnext --mode file src",
        "lint": "eslint ./src ./test",
        "local-audit": "yarn install && yarn npm audit -A -R \"$@\"",
        "checkup": "yarn dlx @yarnpkg/doctor \"$@\"",
        "dependency-check": "yarn dlx depcheck \"$@\"",
        "report-coverage": "cat ./coverage/lcov.info | coveralls",
        "start": "ts-node src/server.ts",
        "test": "jest --coverage --forceExit --runInBand",
        "test:prod": "npm run lint && npm run test -- --no-cache",
        "test:watch": "jest --coverage --watch",
        "postversion": "git push --follow-tags"
    },
    "prettier": {
        "printWidth": 120,
        "semi": true,
        "singleQuote": false,
        "tabWidth": 4,
        "trailingComma": "es5"
    },
    "dependencies": {
        "@composer-js/core": "^6.0.0-rc.12",
        "@composer-js/service-core": "^8.0.0-rc.13",
        "@types/node": "^22.8.4",
        "axios": "^1.7.7",
        "connect-redis": "^7.1.1",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "express": "^4.21.1",
        "express-session": "^1.18.1",
        "ioredis": "^5.4.1",
        "lodash": "^4.17.21",
        "mongodb": "^6.10.0",
        "nconf": "^0.12.1",
        "openapi3-ts": "^4.4.0",
        "passport": "^0.7.0",
        "passport-strategy": "^1.0.0",
        "prom-client": "^15.1.3",
        "reflect-metadata": "^0.2.2",
        "typeorm": "^0.3.20",
        "winston": "^3.15.0",
        "winston-transport": "^4.8.0",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.7",
        "@types/ioredis-mock": "^8.2.5",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4.17.12",
        "@types/nconf": "^0.10.7",
        "@typescript-eslint/eslint-plugin": "^8.12.2",
        "@typescript-eslint/parser": "^8.12.2",
        "coveralls": "^3.1.1",
        "eslint": "^9.13.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsdoc": "^50.4.3",
        "globals": "^15.11.0",
        "ioredis-mock": "^8.9.0",
        "jest": "^29.7.0",
        "jest-junit": "^16.0.0",
        "mkdirp": "^3.0.1",
        "mongodb-memory-server": "^10.1.2",
        "nodemon": "^3.1.7",
        "rimraf": "^6.0.1",
        "supertest": "^7.0.0",
        "superwstest": "^2.0.4",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typedoc": "^0.26.10",
        "typedoc-plugin-markdown": "^4.2.9",
        "typescript": "^5.6.3",
        "uuid": "^11.0.2"
    },
    "resolutions": {
        "axios": "^1.7.7",
        "express": "^4.21.0"
    },
    "packageManager": "yarn@4.2.2",
    "engines": {
        "node": ">=20.0.0"
    }
}
